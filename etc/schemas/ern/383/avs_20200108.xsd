         <xs:enumeration value="Calligrapher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of artistic handwriting or
                  lettering.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CameraOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating a
                  camera.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Carpenter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who carries out
                  woodwork.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Cartographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a map or other cartographic
                  work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Cartoonist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a cartoon.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CastingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party selecting talent (Actors, singers,
                  Dancers, etc.).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Causeur">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who gives an informal lecture,
                  typically on radio.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Censor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Editor who examines Creations and
                  removes objectionable content.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChiefLightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the head of the
                  LightingTechnicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Choir">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of Parties who perform vocally
                  together. Typically, Choirs consist of at least 2 people in an combination of
                  different vocal ranges.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChoirMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A member of a Choir.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Choreographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a dance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChorusMaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Conductor who directs Vocalists in a
                  Choir.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CircusArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist with skills that are typical for
                  circus performances.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ClapperLoader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who operates the clapboard at the
                  beginning of a shot.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ClubDJ">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who announces and plays recorded
                  music in a music club.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director who is sharing some or all of a
                  director's responsabilities with a Director or another
                  CoDirector.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoExecutiveProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An ExecutiveProducer who is not the only
                  ExecutiveProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ColorSeparator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for separating the
                  original artwork into individual color components for printing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Comedian">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist who makes people laugh (by
                  telling funny stories or by acting).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">One of a group of Artists who creates an
                  original master album or single mix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoMixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is sharing some or all of a
                  mixing engineer's responsabilities with a MixingEngineer or another
                  CoMixer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Commentator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who provides interpretation,
                  analysis, or a discussion of a subject matter.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CommissioningBroadcaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party commissioning material for a
                  broadcast programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CompilationProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer responsible for supervising
                  and/or exercising control over the work assigned to other Producers, Engineers,
                  and others on the project.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ComputerGraphicCreator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of computer
                  graphics.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ComputerProgrammer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a computer
                  program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ConcertMaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The leader of the first violin section in
                  an Orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Conductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who leads or conducts a
                  Performance by a group of Musicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Consultant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party giving professional advice on
                  matters of production technique or on content who does not appear in the
                  programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ContinuityChecker">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who checks the continuity of a
                  series of camera shots.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Contractor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is hired to perform
                  work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer who is sharing some or all of a
                  producer's responsabilities with a Producer or another
                  CoProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Correspondent">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the writer of a letter or
                  other communication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CostumeDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a
                  costume.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoverDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a cover.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dancer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a
                  dance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Delineator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of technical drawings derived
                  from designs made by other people.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Designer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a design.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DialogueCoach">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who helps train an Actor in
                  diction and/or the use of dialects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DialogueDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for assembling,
                  synchronizing, and editing all the dialogues in a film or television
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalAudioWorkstationEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer is responsible for the
                  operation of a Digital Audio Workstation (DAW) during recording and mix sessions
                  including the archiving and maintenance of the recorded material (basically data
                  files). The DigitalAudioWorkstationEngineer is not generally called upon to make
                  major aesthetic judgments.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalEditingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for operating
                  equipment for editing (in todayâs world generally a digital audio workstation).
                  They can, from time to time, be called on to make aesthetic
                  choices.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalEditingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a
                  DigitalEditingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Director">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who leads or supervises actors,
                  e.g. in the prodution of a movie.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DirectStreamDigitalEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the functioning
                  and maintenance of digital recordings using Direct Stream Digital format
                  equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DistributionCompany">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Organization responsible for delivering
                  material provided by a ProductionCompany.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DJ">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who announces and plays recorded
                  music.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Draughtsman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a plan for constructing
                  something.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dresser">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who helps an Actor to put on and
                  take off a costume.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dubber">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator who adds the dubbing into a
                  Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Editor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the preparation of
                  a publication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EditorInChief">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The head of editorial
                  staff.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EditorOfTheDay">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the preparation of
                  a news broadcast of the day.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Encoder">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator processing and encoding a
                  Resource for distribution.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Engineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that is responsible for technical
                  aspects. This includes RecordingEngineer, but may also cover roles in other
                  content production processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Ensemble">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of two or more Parties performing
                  a MusicalWork together. Note: An Ensemble may be of any size or any grouping of
                  Performers from a vocal duo to a full orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ExecutiveProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who secures funding for a project.
                  He or she may be responsible for the overall concept. He or she may monitor the
                  project from time to time to ensure that the producer and Artist are on
                  track.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Expert">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who contributes his expertise to a
                  programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Facsimilist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a copy.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FightDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for the
                  choreography of fight scenes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director of a movie.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmDistributor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Publisher of films.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who edits an audio-visual
                  production for publishing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party (individual or company) that is
                  responsible for the financing of an audio-visual production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmSoundEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that is responsible for (part of)
                  the soundtrack used in an audio-visual Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FloorManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party acting on the studio floor on
                  behalf of the Director.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FocusPuller">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CameraOperator who adjusts the focus of
                  the camera.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who creates incidental sound
                  effects in post-production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who edits sound effects created by
                  a FoleyArtist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A SoundMixer who works with a FoleyArtist
                  to create sound effects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a drawing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicAssistant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party drawing pictures or picture
                  elements as content for programmes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of graphical
                  elements.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Greensman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party procuring and maintaining
                  vegetation on a set.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Grip">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supporting CameraOperators and
                  LightingTechnicians with specialized equipment (e.g. camera
                  cranes).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GuestConductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Conductor who is working with an
                  Orchestra on a temporary basis.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GroupMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Person that is a member of a
                  MusicGroup.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Hairdresser">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who prepares the hair of an
                  Actor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Illustrator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of an
                  illustration.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="InitialProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that initiates the creation of a
                  SoundRecording or Resource and is sometimes referred to as a commissioning rights
                  holder. An InitialProducer may be a Person or an Organization and the term
                  contrasts with the role of a StudioProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="InterviewedGuest">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is
                  interviewed.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Interviewer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who interviews, i.e. talks to
                  somebody for the purpose of eliciting his opinions.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyCharacter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A central or primary Character in a
                  storyline.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyGrip">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the head of the
                  Grips.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyTalent">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party performing as a leading
                  character.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Leadman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is responsible for the crew in
                  charge of the set decoration.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LeadPerformer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Performer that plays the role of the
                  protagonist in a film or play.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LeadVocalist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is a lead
                  Vocalist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LightingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the
                  lighting.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating lighting
                  equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LocationManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party managing all aspects relating to
                  filming in specific places.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MakeUpArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who prepares the make up of an
                  Actor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Manufacturer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for manufacturing a
                  device or a piece of technical equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MasteringEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer who is the last creative
                  bridge between the mix process and the distribution process. He or she generally
                  works with a final mix of a project and makes final adjustments to the sound
                  before distribution while maintaining the vision of the artist, producer and
                  label.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MasteringSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MasteringEngineer (aka
                  assistant mastering engineer).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MatteArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who creates artworks used in
                  making scenery and locations that do not exist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Mixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist who creates an original master
                  album or single mix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer combining different audio
                  recording elements into a final version. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MixingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MixingEngineer (aka
                  assistant mixing engineer).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicArranger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Arranger who adds the final touches or
                  subtle nuances to the musical arrangement of a recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicCopyist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a copy of a Composer or
                  Arranger's manuscript.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for music in the
                  production of a film or a radio program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicGroup">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of individuals who perform vocally
                  and/or instrumentally together.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Musician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a MusicalWork either
                  vocally or instrumentally.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Narrator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who tells a story or gives an
                  account of an event.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NewsProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a news
                  program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NewsReader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who reads news
                  stories.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NotSpecified">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type used when a sender of a DdexMessage
                  wishes to indicate that the value within the allowed value set is known, but not
                  specified.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Orchestra">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A large group of Parties performing a
                  MusicalWork together, predominantly using musical instruments rather than voice.
                  An Orchestra is typically led by a Conductor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OrchestraMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A member of an
                  Orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OriginalArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist who is an original
                  Artist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OverdubEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer combining different audio
                  recordings into an overdub.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OverdubSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an
                  OverdubEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Painter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a
                  painting.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Performer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A performer of a Work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Photographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a
                  Photograph.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PhotographyDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for photography
                  (aka director of photgraphy or cinematographer).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PlaybackSinger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who sings songs to be mimed in
                  films by Actors.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PostProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer responsible for post-production
                  processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PreProduction">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer responsible for pre-production
                  processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PreProductionEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer involved in pre-production
                  processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PreProductionSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a
                  PreProductionEngineer</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PrimaryMusician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a MusicalWork either
                  vocally or instrumentally and would be considered the principal Contributor for
                  the piece.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionAssistant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that provides only administrative
                  support for a Producer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionCompany">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Organization responsible for a
                  production (also called a production house).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionCoordinator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who coordinates the activities
                  around preparing a physical or digital Release.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionDepartment">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A department of a ProductionCompany
                  responsible for a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Person who coordinates a
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionSecretary">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party providing secretarial support in a
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProjectEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer involved in a recording
                  Project.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Programmer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party using electronic devices, such as
                  sequencers, to generate sounds of musical Instruments in a music
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProgrammingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the functioning
                  and maintenance of digital recorders and digital audio workstations.
               </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProgramProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a program, e.g. a TV or
                  radio program. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PropertyManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for purchasing,
                  acquiring and/or manufacturing anything needed for a
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PublishingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the coherence and
                  continuity of a publication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Puppeteer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who operates puppets in a
                  Performance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Pyrotechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating equipment to produce
                  fire and explosions.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RecordingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supervising the technical aspects
                  of a sound or video RecordingSession.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RecordingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a RecordingEngineer (aka
                  assistant recording engineer).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Redactor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Editor who prepares text for
                  publication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ReissueProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a reissue of a
                  SoundRecording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RemixedArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist whose work has been
                  remixed.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Remixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist who creates a finished product
                  that is different from the original master album or single mix, e.g. a Dance
                  Remix, Radio Remix, Rock Remix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RemixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A MixingEngineer who creates a finished
                  product that is different from the original master album or single mix, e.g. a
                  Dance Remix, Radio Remix, Rock Remix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RemixingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a
                  RemixingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Repetiteur">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who coaches ballet Dancers or
                  opera singers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Researcher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for researching a
                  project (e.g. gathering ideas, background information etc.).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ResearchTeamHead">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The head of a team of
                  Researchers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ResearchTeamMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A member of a team of
                  Researchers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Restager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for restaging a Work
                  (with minimal contribution to the content).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Rigger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Grip operating equipment to satisfy
                  rigging needs on a set.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RightsControllerOnProduct">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A RightsController as it appears on a
                  Product.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Runner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is a junior member of the
                  production team doing all kinds of supporting jobs.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ScenicOperative">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party in charge of a studio
                  set.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ScientificAdvisor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party providing scientific
                  advice.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ScriptSupervisor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who tracks the progress of a
                  production based on a script.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SecondAssistantCameraOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an
                  AssistantCameraOperator.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SecondAssistantDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an
                  AssistantDirector.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SecondConductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A subordinate to a
                  Conductor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A subordinate to an
                  Engineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SecondUnitDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director in charge of the second unit in
                  a film production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SeriesProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party (individual or company) that is
                  responsible for the production of a series.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SetDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer, also known as a stage
                  designer, who is in charge of designing and creating the sets that appear in
                  films, on television programmes as well as in the theatre.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SetDresser">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party in charge of decorating a set in a
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SignLanguageInterpreter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party translating spoken words into sign
                  language.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Soloist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs the featured Part of
                  a MusicalWork (or a section of it) alone or with only supporting accompaniment.
               </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SoundDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for sound
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SoundMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating audio equipment to
                  capture sound.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SoundRecordist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating audio equipment to
                  record sound.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SoundSupervisor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party oversees the work of all sound
                  personnel on multi-camera productions. During live programmes they are responsible
                  for mixing sound sources for immediate transmission.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Speaker">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs using his voice
                  without singing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SpecialEffectsTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party creating special
                  effects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Sponsor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party giving financial support for an
                  Event or a Creation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StageAssistantEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a
                  StageEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StageDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director of a
                  stageplay.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StageEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Party, also called the monitor
                  engineer, who is responsible for the operation of the monitor mixing console,
                  maintenance and repair of a band's in-ears, onstage, and backline
                  gear.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StoryTeller">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Performer relaying a story with dramatic
                  or theatrical interpretation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StringEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for the process of
                  recording an Orchestra on a project and dealing with technical issues and
                  decisions. The StringEngineer operates the recording equipment (or oversees the
                  operation of the recording equipment) during orchestral Performances and makes
                  aesthetic choices in order to get the sound and concepts the artist and producer
                  are looking for.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StringProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A StudioProducer working with the
                  individual instruments/elements of the recording and not the overall
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StringsDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A director in charge of the performance of
                  the strings section of an Orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StudioConductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party working as a Conductor in a
                  studio.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StudioMusician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a MusicalWork either
                  vocally or instrumentally in a studio.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StudioPersonnel">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is employed in a studio and
                  contributes to the making of a Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StudioProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who directs, and has overall
                  creative and technical oversight of, the entire recording project and the
                  individual recording sessions that are a part of the project. The StudioProducer
                  participates in and/or supervises the recording session and works directly with
                  the Artist, Musicians and Engineers. The StudioProducer makes creative, technical
                  and aesthetic decisions that realize the goals of both the Artist and the
                  SoundRecording CopyrightHolder in the creation of musical content. The producer
                  may perform, direct Performances, choose final takes or versions, and oversees the
                  selection of songs, Musicians, singers, Arrangers, studios, etc. The
                  StudioProducer in collaboration with the artist, assigns credits to Performers and
                  technical personnel, and is responsible for supplying accurate crediting
                  information to the record label or media company as official documentation. Other
                  duties of the StudioProducer may include, but are not limited to, overseeing other
                  staffing needs, keeping budgets and schedules, adhering to deadlines, supervising
                  mastering and overall quality control. Note: a ddexStudio:Producer is a Person and
                  thus may contrast with the role of an InitialProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Stunts">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party performing
                  stunts.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SubtitlesEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that is responsible for the
                  creation of SubTitles/captions.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SubtitlesTranslator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Translator of
                  SubTitles.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SupportingActor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Actor playing a secondary
                  role.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SurroundMixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A MixingEngineer responsible for
                  SurroundSound.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SurroundMixingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MixingEngineer
                  responsible for SurroundSound.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TapeOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party in charge of cueing and preparing
                  video inserts into a program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TechnicalDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for technical aspects
                  of a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Tonmeister">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer who is a graduate of an
                  official Tonmeister program that includes score reading and electronics training
                  as well education in recording, mixing and editing techniques.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TrackingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supervising the technical aspects
                  of a tracking Session.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TrackingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a
                  TrackingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TransfersAndSafetiesEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supervising the technical aspects
                  of a transfers and safeties Session.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TransfersAndSafetiesSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a
                  Transfers&amp;SafetiesEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TransportationManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for managing drivers
                  and co-ordinating the transportation in a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Treatment/ProgramProposal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party writing summaries of various
                  aspects of a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TypeDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a
                  typeface.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Unknown">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity used when a sender of
                  a DdexMessage wishes to indicate that the value within the allowed value set is
                  unknown.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UserDefined">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity which is defined by a
                  sender of a DdexMessage in a manner acceptable to its
                  recipient.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VideoDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director of a Video.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Videographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party in charge of a video
                  production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VideoMusicalDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for a music video
                  production. The VideoMusicalDirector conceives artistic and dramatic aspects while
                  instructing the musical act, technical crew, actors, models, and
                  dancers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VideoProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party (individual or company) that is
                  responsible for the financing of a Video.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VisionMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating video equipment to mix
                  video feeds.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VisualEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Editor who prepares video or film
                  footage.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VisualEffectsTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party producing visual
                  effects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VocalArranger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Arranger who adds the final touches or
                  subtle nuances to the vocal arrangement of a recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VocalEditingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for editing the
                  vocal elements of a recording.</xs:documentation>
            </xs:annotation>
