  <xs:complexType name="RightsController">
    <xs:annotation>
      <xs:documentation source="ddex:Definition">A Composite containing details of a
            RightsController. RightsControllers are typicallydescribed by Name, Identifier and
            role(s).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="PartyId" maxOccurs="unbounded" type="ern:PartyId">
          <xs:annotation>
            <xs:documentation source="ddex:Definition">A Composite containing details of the
