            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PhonoPreamp">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The phono preamplifier used in the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ReferenceClock">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The reference clock used in the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ReferenceTones">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The printed test tones used to align the analog tape machine used in the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SamplingRate">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The sampling rate of the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Side1Condition">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The condition of Side 1 (Face 1) the source material used in the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Side2Condition">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The condition of Side 2 (Face 2) the source material used in the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NumberOfSides">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number of Sides of the source material used in the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SmpteFrameRate">
            <xs:annotation>
