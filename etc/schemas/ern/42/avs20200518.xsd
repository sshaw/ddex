         <xs:enumeration value="SourceMachine">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The source machine used in the Transfer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Speed">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The speed of the source machine used in the Transfer. 30 ips or 33 rpm are common terms.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Storage">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The storage of the material used in the Transfer. 'Tails Out' and 'Heads Out' are common terms.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Stylus">
