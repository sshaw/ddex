  <xs:complexType name="ReferenceTitle">
    <xs:annotation>
      <xs:documentation source="ddex:Definition">A Composite containing details of a
            ReferenceTitle.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TitleText" type="ern:TitleText">
