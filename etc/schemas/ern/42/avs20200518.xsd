            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version with only vocal music.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AlbumVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version produced for an Album.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AlternativeVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An alternative Version of a song other than the originally released or most well-known one. Typically released by the original artist. Often contains many of the same tracks and elements of the main version.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CleanVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version that does not contain any offending elements.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DemoVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version produced for demonstration purposes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EditedVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version that has been edited (no details specified).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="InstrumentalVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version with only instrumental music (lead vocals removed).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KaraokeVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version produced for karaoke.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LiveVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version recorded at a live performance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MixVersion">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Version that is a mix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MonoVersion">
            <xs:annotation>
