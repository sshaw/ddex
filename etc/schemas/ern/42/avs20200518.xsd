            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number of channels used in the Transfer. 'Stereo' and 'Mono' are common terms.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ClockSource">
            <xs:annotation>
