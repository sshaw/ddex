               <xs:documentation source="ddex:Definition">A very slow tempo (25–45 bpm).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Larghetto">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A rather broad tempo (60–66 bpm).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Larghissimo">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A very, very slow tempo (24 bpm and under).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Largo">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A broad tempo (40–60 bpm).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Lento">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A slow tempo (45–60 bpm).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MarciaModerato">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A moderate tempo, in the manner of a march (83–85 bpm).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Moderato">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A tempo at a moderate speed (108–120 bpm).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Prestissimo">
            <xs:annotation>
